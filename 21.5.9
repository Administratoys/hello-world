#include <stdio.h>
int main() 
{
	int a[11] = { 1,5,8,9,25,26,31,35,56,64 };
	int m, end, temp1, temp2;
	int i, j;
	printf("请输入要插入的数:");
	scanf_s("%d", &m);
	end = a[9];
	if (m >= end) 
	{
		a[10] = m;
	}
	else 
	{
		for (i = 0; i < 10; i++) 
		{
			if (a[i] > m) 
			{
				temp1 = a[i];
				a[i] = m;
				for (j = i + 1; j < 11; j++) 
				{
					temp2 = a[j];
					a[j] = temp1;
					temp1 = temp2;
				}
				break;
			}
		}
	}
	printf("新的数组为：");
	for (i = 0; i < 11; i++) 
	{
		printf("  %d", a[i]);
	}
	return 0;
}
